{
  "name": "actuar",
  "version": "1.1.2",
  "description": "A logger that provides many options to log data and informations",
  "main": "./dist/Actuar.js",
  "typings": "./dist/Actuar.d.ts",
  "scripts": {
    "pub": "tsc -p ./tsconfig.publish.json && npm run test",
    "build": "tsc -p ./tsconfig.json",
    "test": "npm run build_test && mocha ./test/build/**/*.test.js --reporter spec --exit",
    "watch": "tsc -p ./tsconfig.json --watch",
    "build_test": "tsc -p ./tsconfig.test.json",
    "watch_test": "tsc -p ./tsconfig.test.json --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiltscher/actuar.git"
  },
  "keywords": [
    "logger",
    "log",
    "logging",
    "debug",
    "console",
    "warn",
    "error"
  ],
  "author": "Christian Hiltscher",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/chiltscher/actuar/issues"
  },
  "homepage": "https://github.com/chiltscher/actuar#readme",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.11.1",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.1",
    "@types/pug": "^2.0.4",
    "mocha": "^5.0.5",
    "typedoc": "^0.11.1"
  },
  "dependencies": {
    "@types/chai": "^4.1.2",
    "chai": "^4.1.2",
    "chalk": "^2.3.2",
    "express": "^4.16.3",
    "js-doc": "^0.5.0",
    "npm": "^5.8.0",
    "pug": "^2.0.3"
  }
}
